    {
      "id": "validate-data",
      "name": "Validar Dados",
      "type": "n8n-nodes-base.function",
      "position": [300, 200],
      "parameters": {
        "functionCode": "// Validar dados obrigat√≥rios\nconst requiredFields = ['name', 'whatsapp', 'storeUrl', 'monthlyRevenue', 'platform', 'trafficInvestment', 'agreement'];\n\nfor (const field of requiredFields) {\n  if (!$json.body[field]) {\n    throw new Error(`Campo obrigat√≥rio ausente: ${field}`);\n  }\n}\n\n// Limpar e formatar dados\nconst cleanData = {\n  name: $json.body.name.trim(),\n  whatsapp: $json.body.whatsapp.replace(/\\D/g, ''), // Remove caracteres n√£o num√©ricos\n  store_url: $json.body.storeUrl.toLowerCase().trim(),\n  monthly_revenue: $json.body.monthlyRevenue,\n  platform: $json.body.platform,\n  traffic_investment: $json.body.trafficInvestment,\n  agreement: $json.body.agreement,\n  source: $json.body.source || 'landing_page',\n  domain: $json.body.domain,\n  user_agent: $json.body.userAgent,\n  referrer: $json.body.referrer,\n  session_id: $json.body.sessionId\n};\n\nreturn [{ json: cleanData }];"
      }
    },
    {
      "id": "supabase-insert",
      "name": "Supabase - Inserir Lead",
      "type": "n8n-nodes-base.supabase",
      "position": [500, 200],
      "parameters": {
        "operation": "insert",
        "table": "leads",
        "fieldsUi": {
          "fieldValues": [
            {
              "fieldId": "name",
              "fieldValue": "={{ $json.name }}"
            },
            {
              "fieldId": "whatsapp",
              "fieldValue": "={{ $json.whatsapp }}"
            },
            {
              "fieldId": "store_url",
              "fieldValue": "={{ $json.store_url }}"
            },
            {
              "fieldId": "monthly_revenue",
              "fieldValue": "={{ $json.monthly_revenue }}"
            },
            {
              "fieldId": "platform",
              "fieldValue": "={{ $json.platform }}"
            },
            {
              "fieldId": "traffic_investment",
              "fieldValue": "={{ $json.traffic_investment }}"
            },
            {
              "fieldId": "agreement",
              "fieldValue": "={{ $json.agreement }}"
            },
            {
              "fieldId": "source",
              "fieldValue": "={{ $json.source }}"
            },
            {
              "fieldId": "domain",
              "fieldValue": "={{ $json.domain }}"
            },
            {
              "fieldId": "user_agent",
              "fieldValue": "={{ $json.user_agent }}"
            },
            {
              "fieldId": "referrer",
              "fieldValue": "={{ $json.referrer }}"
            },
            {
              "fieldId": "session_id",
              "fieldValue": "={{ $json.session_id }}"
            }
          ]
        }
      }
    },
    {
      "id": "check-score",
      "name": "Verificar Score",
      "type": "n8n-nodes-base.if",
      "position": [700, 200],
      "parameters": {
        "conditions": {
          "number": [
            {
              "value1": "={{ $json.score }}",
              "operation": "largerEqual",
              "value2": 70
            }
          ]
        }
      }
    },
